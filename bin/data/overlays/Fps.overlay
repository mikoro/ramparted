FpsOverlay
{
	zorder 100
	
	container Panel(FpsPanel)
	{
		metrics_mode pixels
		width 121
		height 36
		material FpsPanelMaterial
		
		element TextArea(RenderFpsTextArea)
		{
			metrics_mode pixels
			left 6
			top 4
			width 200
			height 30
			font_name DejaVuSansMono
			char_height 16
		}
		
		element TextArea(LogicFpsTextArea)
		{
			metrics_mode pixels
			left 6
			top 20
			width 200
			height 30
			font_name DejaVuSansMono
			char_height 16
		}
	}
}

